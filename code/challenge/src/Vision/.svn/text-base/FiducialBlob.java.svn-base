package Vision;

import java.awt.Color;

public class FiducialBlob {
	// State Variables
	public Color[] colors;
	public int size;
	public int x;
	public int y;

	public FiducialBlob(Color[] colors, int size, int x, int y){
		this.colors = colors;
		this.size = size;
		this.x = x;
		this.y = y;
	}
}